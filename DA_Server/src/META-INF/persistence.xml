    <class>vn.com.dva.entities.ResultExam</class>
    <class>vn.com.dva.entities.DetailResultExam</class>
    <class>vn.com.dva.entities.DetailTrain</class>
    <class>vn.com.dva.entities.News</class>
    <class>vn.com.dva.entities.config</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:h2:tcp://localhost/~/TESTONLINE"/>
      <property name="javax.persistence.jdbc.password" value=""/>
